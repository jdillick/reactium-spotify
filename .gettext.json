{
    "js": {
        "parsers": [
            {
                "expression": "gettext",
                "arguments": {
                    "text": 0
                }
            },
            {
                "expression": "__",
                "arguments": {
                    "text": 0
                }
            },
            {
                "expression": "ngettext",
                "arguments": {
                    "text": 0,
                    "textPlural": 1
                }
            },
            {
                "expression": "_n",
                "arguments": {
                    "text": 0,
                    "textPlural": 1
                }
            },
            {
                "expression": "pgettext",
                "arguments": {
                    "context": 0,
                    "text": 1
                }
            }
        ],
        "glob": {
            "pattern": "src/app/**/*.js"
        }
    },
    "headers": {
        "Language": "en_US"
    },
    "output": "src/reactium-translations/template.pot"
}
